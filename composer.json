{
    "name": "unit-conversions/perform-unit-conversions-in-php",
    "description": "This is my package PerformUnitConversionsInPhp",
    "keywords": [
        "unit-conversions",
        "perform-unit-conversions-in-php"
    ],
    "homepage": "https://github.com/unit-conversions/perform-unit-conversions-in-php",
    "license": "MIT",
    "authors": [
        {
            "name": "gekongfei",
            "email": "389443626@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
